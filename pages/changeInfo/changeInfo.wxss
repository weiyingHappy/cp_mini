/* pages/changeInfo/changeInfo.wxss */
.change_title{
  padding:32rpx 26rpx 24rpx;
}
.change_title input{
  padding: 24rpx 20rpx;
  font-size:28rpx;
  color: #464646;
  border-bottom: 2rpx solid #26888E;
}
.save{
  padding:32rpx 0;
  display: flex;
  justify-content: flex-end;
}
.save button{
  font-size:28rpx;
  background: #26888E;
  color: #fff;
  width: 140rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  border-radius: 36rpx;
}