/* pages/invoice/invoice.wxss */

.invoice_list{
  margin-top: 20rpx;
}
.invoice_list .invoice_item:last-child {
  border: none;
}
.invoice_item {
  padding: 30rpx;
  background: #fff;
  font-size: 28rpx;
  color: #404040;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: solid 1rpx #e9e9e9;
}
.invoice_item_left {
  display: flex;
  align-items: center;
}
.invoice_item image {
  width: 28rpx;
  height: 28rpx;
}